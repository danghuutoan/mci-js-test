    "def solution(s):\n",
    "    input = s.split(':')\n",
    "    col1, row1 = input[0]\n",
    "    col2, row2 = input[1]\n",
    "    for col in range(ord(col1), ord(col2) + 1):\n",
    "        for row in range(int(row1), int(row2) + 1):\n",
    "            print(f\"{chr(col)}{row}\")\n"
   ]
